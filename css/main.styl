.container-modal-header{
  position: relative;
  width: 49%;
  float: right;
  height: 20px;
}
.modal { text-align: center; padding: 0!important; } .modal:before { content: ''; display: inline-block; height: 100%; vertical-align: middle; margin-right: -4px; } .modal-dialog { display: inline-block; text-align: left; vertical-align: middle; }

.modal-body{
  background: black;
  width : initial;
}
.modal-body .imageContainer
  display  block


.modal-body img{
  width : auto;
  height: auto;
  max-height : 500px;
  max-width :100%;
  margin : auto;
  vertical-align: middle;
  position: absolute;
  top: 0;
  bottom : 0;
  right : 0;
  left : 0;
}
.modal-body figure{
  width : 100%;
  height: 500px;
  position : relative;
  display : table;
}
.container-modal-header span, .container-modal-header .modalTitleRed button{
  position: absolute;
  color : black;
  top: 0;
  right: 0;
  background-color: transparent;
  border: none;
}
